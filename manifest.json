{
  "manifest_version": 3,
  "name": "Code Snippet Clipboard",
  "version": "1.0",
  "description": "A Chrome extension to manage and insert code snippets.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
